{
    "ServerExtraOption":
    {
        "UseDefaultSuperUser": false,
        "ServerEnv": "Dev",
        "DateTimeVisibleType": "yyyy-MM-dd HH:mm:ss"
    },
    "Serilog":
    {
        "Using":
        [
            "Serilog.Sinks.Console",
            "Serilog.Sinks.File"
        ],
        "MinimumLevel":
        {
            "Default": "Information",
            "Override":
            {
                "Microsoft": "Warning",
                "Microsoft.AspNetCore.Mvc": "Warning",
                "Microsoft.AspNetCore.Routing": "Warning",
                "Microsoft.AspNetCore.Server.Kestrel": "Warning",
                "Microsoft.AspNetCore.Hosting.Diagnostics": "Information",
                "Microsoft.Hosting.Lifetime": "Information"
            }
        },
        "WriteTo":
        [
            {
                "Name": "Console",
                "Args":
                {
                    "restrictedToMinimumLevel": "Debug"
                }
            },
            {
                "Name": "File",
                "Args":
                {
                    "path": "Logs/log_.log",
                    "rollingInterval": "Day"
                }
            }
        ]
    },
    "AllowedHosts": "*"
}